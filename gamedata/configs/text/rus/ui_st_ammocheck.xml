    <!-- Animation Settings -->
    <string id="ui_mcm_rax_ammo_check_enable_animations">
        <text>Включить анимации</text>
    </string>
    <string id="ui_mcm_rax_ammo_check_enable_animations_desc">
        <text>Главный переключатель анимаций. Если выкл. - только текстовое сообщение без анимации оружия.</text>
    </string>
    <string id="ui_mcm_rax_ammo_check_anim_mode">
        <text>Анимация осмотра магазина</text>
    </string>
